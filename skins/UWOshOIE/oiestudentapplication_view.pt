<html xmlns="http://www.w3.org/1999/xhtml"
          xmlns:tal="http://xml.zope.org/namespaces/tal"
          xmlns:metal="http://xml.zope.org/namespaces/metal"
          xmlns:i18n="http://xml.zope.org/namespaces/i18n"
          i18n:domain="plone">
      <body>

        <h1 metal:define-macro="header" 
            tal:define="  fName here/firstName|nothing; 
                        mName here/middleName|nothing; 
                        lName here/lastName|nothing; 
                        pYear here/programYear|nothing; 
                        pSemester here/programSemester|nothing;
                        programName here/getProgramName|nothing;
                        pName python: programName != None and programName.Title();"
            tal:content="string: $fName $mName $lName" >
            [full name]
        </h1>

        <div    id="body" 
                metal:define-macro="body"
                tal:define="field_macro field_macro | here/widgets/field/macros/view;
                            review_state wf_state;">

<div id="UWOshOIEApplication">
    
    <div    class="portalMessage" 
            tal:condition="python: review_state and review_state == 'private'">
        Your application is not submitted yet.  You must click the "Submit app" button in the "Application State" area to submit to the OIE staff
    </div> 

<div id="UWOshOIEOverview">

	<div id="UWOshOIEBasicInfo">
    	<fieldset><legend>Basic Information</legend>
        	<div 	class="field ArchetypesField-StringField"
                    id="archetypes-fieldname-studentID">
          		<label>
					<span>
						UW Oshkosh Student ID
					</span>
						:
				</label>
          		<span tal:content="here/studentID">
					[studentID]
				</span> 
        	</div>
        
        	<div 	class="field ArchetypesField-StringField"
                    id="archetypes-fieldname-email">
          		<label>
					<span>
						Email Address
					</span>
						:
				</label>
          		<span 	tal:condition="here/email" 
						tal:replace="structure string:<a href='mailto:${here/email}'>${here/email}</a>">
					[email address]
				</span> 
			</div>
          
        	<div 	class="field ArchetypesField-StringField"
             		id="archetypes-fieldname-programName"
					tal:define="programName here/getProgramName|nothing;
								pName programName/Title|nothing;">
          		<label>
					<span width="300px">
						Program Name
					</span>
						:
				</label>
          		<span tal:content="pName">
					[programName]
          		</span>  
			</div>
          
        	<div 	class="field ArchetypesField-IntegerField"
					id="archetypes-fieldname-programYear"
            		tal:define="pYear here/programYear|nothing;
                        		pSem here/programSemester|nothing;">
          		<label>
					<span>
						Program Semester &amp; Year 
					</span>
						:
				</label>
          		<span tal:content="string:$pSem $pYear">
					[programSemester and programYear]
          		</span>  
			</div>
    	</fieldset>
	</div>

	<div id="UWOshOIEApplicationState">
    	<fieldset>
			<legend>Application State</legend>
        	<p tal:condition="review_state">
            	<span tal:attributes="class string:state-${review_state}" size=+2>Your application state is: <span tal:content="review_state"/></span>
        	</p>
        	<p>
            	<span tal:replace="python: context.get_workflow_state_description(wf_state)"/>  
        	</p>
        	<p>
            	<a href="application-states-and-workflow">See process overview</a>
        	</p>
        	<tal:block>
  				<div id="UWOshOIEFunkyButtons">
            		<ul>
<li>
<tal:block condition="python: review_state and review_state == 'facultyReview'" define="programs python: context.getRefs(); program_title python: len(programs) > 0 and programs[0].title_or_id() or 'no program selected'">


<form method="post" action="https://plonedev.uwosh.edu/oie/applicant-approval-form">
<input type="hidden" name="student-name" tal:attributes="value python:context.firstName + ' ' + context.middleName + ' ' + context.lastName" />
<input type="hidden" name="student-email-address" tal:attributes="value python:str(context.email)" />
<input type="hidden" name="academic-and-career-plan" tal:attributes="value python:str(context.questionAcadCareerPlan)" />
<input type="hidden" name="language-and-cultural-skills" tal:attributes="value python:str(context.questionLangCulturalSkills)" />
<input type="hidden" name="previous-travel-experience" tal:attributes="value python:str(context.questionPrevTravel)" />
<input type="hidden" name="work-experience" tal:attributes="value python:str(context.questionWorkExp)" />
<input type="hidden" name="european-business-travel-seminar-only" tal:attributes="value python:str(context.questionEuroBizTravSem)" />
<input type="hidden" name="student-exchange-and-competitive-programs-only" tal:attributes="value python:str(context.questionStuExchComp)" />
<input type="hidden" name="student-type" tal:attributes="value python:str(context.studentType)" />
<input type="hidden" name="name-of-other-university" tal:attributes="value python:str(context.universityEnrolled)" />
<input type="hidden" name="expected-graduation-month-1" tal:attributes="value python:str(context.graduationMonth)" />
<input type="hidden" name="expected-graduation-year" tal:attributes="value python:str(context.graduationYear)" />
<input type="hidden" name="cumulative-gpa" tal:attributes="value python:str(context.cumulativeGPA)" />
<input type="hidden" name="first-major-1" tal:attributes="value python:str(context.major1)" />
<input type="hidden" name="second-major-1" tal:attributes="value python:str(context.major2)" />
<input type="hidden" name="minor-1" tal:attributes="value python:str(context.minor1)" />
<input type="hidden" name="minor-2" tal:attributes="value python:str(context.minor2)" />
<input type="hidden" name="emphasis-licensure-1" tal:attributes="value python:str(context.emphasis1)" />
<input type="hidden" name="emphasis-licensure-2" tal:attributes="value python:str(context.emphasis2)" />
<input type="hidden" name="course-1-subject" tal:attributes="value python:str(context.subject1)" />
<input type="hidden" name="course-number-1" tal:attributes="value python:str(context.course1)" />
<input type="hidden" name="credits-1" tal:attributes="value python:str(context.credits1)" />
<input type="hidden" name="course-2-subject" tal:attributes="value python:str(context.subject2)" />
<input type="hidden" name="course-number-2" tal:attributes="value python:str(context.course2)" />
<input type="hidden" name="credits-2" tal:attributes="value python:str(context.credits2)" />
<input type="hidden" name="course-3-subject" tal:attributes="value python:str(context.subject3)" />
<input type="hidden" name="course-number-3" tal:attributes="value python:str(context.course3)" />
<input type="hidden" name="credits-3" tal:attributes="value python:str(context.credits3)" />
<input type="hidden" name="course-4-subject" tal:attributes="value python:str(context.subject4)" />
<input type="hidden" name="course-number-4" tal:attributes="value python:str(context.course4)" />
<input type="hidden" name="credits-4" tal:attributes="value python:str(context.credits4)" />
<input type="hidden" name="course-5-subject" tal:attributes="value python:str(context.subject5)" />
<input type="hidden" name="course-number-5" tal:attributes="value python:str(context.course5)" />
<input type="hidden" name="credits-5" tal:attributes="value python:str(context.credits5)" />
<input type="hidden" name="course-6-subject" tal:attributes="value python:str(context.subject6)" />
<input type="hidden" name="course-number-6" tal:attributes="value python:str(context.course6)" />
<input type="hidden" name="credits-6" tal:attributes="value python:str(context.credits6)" />
<input type="hidden" name="expectations-1" tal:attributes="value python:str(context.questionExpectations)" />
<input type="hidden" name="this-students-g.p.a.-is" tal:attributes="value python:str(context.cumulativeGPA)" />
<input type="hidden" name="program-semester-year" tal:attributes="value python:str(context.programSemester)" />
<input type="hidden" name="program-year" tal:attributes="value python:str(context.programYear)" />
<input type="hidden" name="program-name" tal:attributes="value python:str(program_title)" />
<input type="hidden" name="student-app-url" tal:attributes="value python:str(context.absolute_url())" />


<input type="submit" value="Populate Form Data" />
</form>

</tal:block>
</li>
                		<li>
                    		<a tal:condition="is_editable" 
							   tal:attributes="href python:context.absolute_url() + '/base_edit?last_referer=' + context.absolute_url()">
                        		Edit application
                    		</a>
                		</li>

                		<tal:block repeat="action wf_actions"
									tal:define="url_for_action string:$here_url/confirm_content_status_modify">
                    		<li tal:define="transition python: context.get_transition_information(action);
											action_title transition/title;
										    action_id transition/id;
											action_description transition/description"
								tal:condition="python: action_id != 'addComment'">
                        		<a tal:attributes="href python: url_for_action + '?' + context.url_encode({'transition_title': action_title, 'transition_id': action_id, 'transition_description': action_description})"
								   tal:content="action_description">
                            		The Transition Title
								</a>
                    		</li>
                		</tal:block>
            		</ul>
  				</div>
        	</tal:block>
    	</fieldset>
	</div>
</div>

<div id="UWOshOIExpandCollapseAll">
	<form>
		<input type="button" onclick="expandCollapsibles();" value="Expand all sections">
		<input type="button" onclick="collapseCollapsibles();" value="Collapse all sections">
	</form>
</div>

<div id="UWOshOIEApplicationHistory">
	<fieldset>
		<legend>Comments & History</legend>
		<div>
			<tal:block repeat="action wf_actions">
				<div tal:define="action_title action/title" tal:condition="python:action_title.startswith('addComment')">
					<span class="AddComments">
						<dl id="Addresses" class="collapsible collapsedOnLoad">
							<dt class="collapsibleHeader">Add Comment</dt>
							<dd class="collapsibleContent">
								<form tal:attributes="action string:$here_url/content_status_modify" method="get">
									<label i18n:translate="label_comments">Comment to add:</label>
									<textarea 	id="comment"
                                              	name="comment"
												cols="60"
												rows="3"
												tabindex=""
												tal:attributes="tabindex tabindex/next;"
												tal:content="request/comment|nothing">
									</textarea>
									<input type="hidden" name="workflow_action" value="addComment" />
									<input type="submit" value="Add comment" /> 
								</form>     
							</dd>
						</dl>
					</span>
				</div>
			</tal:block>
		</div>
		<span metal:use-macro="here/uwoshoie_review_history/macros/uwoshoie_review_history" />
	</fieldset>
</div>

